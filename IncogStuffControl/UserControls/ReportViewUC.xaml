<UserControl x:Class="IncogStuffControl.UserControls.ReportViewUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IncogStuffControl.UserControls"
            xmlns:rdlcreport="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"  
        mc:Ignorable="d"  
         Height="auto" Width="auto">
    <Grid HorizontalAlignment="Stretch" Margin="0,-20,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <WindowsFormsHost Grid.Column="0" Height="600" Width="900" Name="windowsFormsHost1" Grid.Row="0" >
            <rdlcreport:ReportViewer x:Name="_reportviewer" />
        </WindowsFormsHost>
        <Canvas Height="27" Grid.Column="0" Grid.Row="1" Name="canvas1" Width="176" HorizontalAlignment="Right" >
            <Button Name="btnCancel" Height="45" Width="75" Content="Cancel" Click="btnCancel_Click" Canvas.Left="-90" Canvas.Top="0" TabIndex="7"></Button>
        </Canvas>
    </Grid>
</UserControl>
